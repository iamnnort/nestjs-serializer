  "devDependencies": {
    "@iamnnort/nestjs-logger": "^1.4.1",
    "@nestjs/core": "^11.0.16",
    "@nestjs/platform-express": "^11.0.16",
    "@swc/core": "^1.11.20",
    "@tsconfig/node20": "^20.1.5",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
